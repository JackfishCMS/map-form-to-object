{
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "plugins": ["prettier"],
  "rules": {
    "object-curly-spacing": ["error", "always"],
    "array-bracket-spacing": ["error", "always"]
  },
  "env": {
    "es6": true,
    "node": true,
    "jest": true,
    "browser": true,
    "jquery": true
  },
  "globals": {}
}
